  "main": "./lib/index.cjs.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
